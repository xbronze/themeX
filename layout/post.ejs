<!-- 博客详情 -->

<h2><%- page.title %></h2>
<p><%- date(page.date , 'YYYY/MM/DD HH:mm:ss') %> </p>
<%- page.content  %> 

<h5>
    <% if (page.prev != null) { %>
        <a href="<%- url_for(page.prev.path) %>">
            < 上一篇：<%- page.prev.title  %>
        </a>
    <% } %>
    
</h5>

<h5>
    <% if (page.next != null) { %>
        <a href="<%- url_for(page.next.path) %>">
            下一篇：<%- page.next.title  %> >
        </a>
    <% } %>
    
</h5>
